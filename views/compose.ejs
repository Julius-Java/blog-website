<%- include("./partials/header") %> 
    <h1>Compose</h1>
    <form action="/compose" method="post">
        <div class="mb-3 form-group">
            <label for="postHeading" class="form-heading">Title</label>

            <input type="text" class="form-control" name="postHeading">
        </div>

        <div class="mb-3 form-group">
            <label for="postBody" class="form-heading">Post</label>
            <textarea class="form-control" name="postBody" id="postText"  rows="5"></textarea>
        </div>

        <button type="submit" class="btn btn-primary mt-3">Publish</button>
    </form>
<%- include("./partials/footer") %> 